<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.yedam.mealkit.member.mapper.MemberMapper">

 	<select id="memberSelectList" resultType="co.yedam.mealkit.member.service.MemberVO">
 		SELECT * FROM MEMBER
 	</select>
 	
 	<select id="memberSelect" resultType="co.yedam.mealkit.member.service.MemberVO">
 		SELECT * FROM MEMBER WHERE MEMBER_ID = #{memberId}
 	</select>
 	
 	<insert id="memberInsert" parameterType="co.yedam.mealkit.member.service.MemberVO">
 		INSERT INTO MEMBER(MEMBER_ID,MEMBER_PASSWORD,MEMBER_NAME,MEMBER_EMAIL)
 		VALU\ES(#{memberId},#{memberPassword},#{memberGrade},#{memberEmail})
 		
 	</insert>
 	
 	
</mapper>